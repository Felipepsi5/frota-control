{
  "name": "frotacontrol",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration=production --optimization --source-map=false",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss}\"",
    "format": "prettier --write \"src/**/*.{ts,html,scss}\"",
    "serve:ssr:frotacontrol": "node dist/frotacontrol/server/server.mjs",
    "json-server": "json-server --watch db.json --port 3000",
    "json-server:routes": "json-server --watch db.json --port 3000",
    "dev": "concurrently \"npm run json-server\" \"npm start\"",
    "dev:routes": "concurrently \"npm run json-server:routes\" \"npm start\"",
    "docker:build": "docker build -t frotacontrol:latest --build-arg NODE_ENV=production --target production .",
    "docker:run": "docker run -p 80:80 -p 443:443 frotacontrol:latest",
    "deploy:check": "npm run lint && npm run test:ci && npm run build:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/fire": "^19.2.0",
    "@angular/forms": "^19.0.0",
    "@angular/material": "^19.2.19",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/platform-server": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@angular/ssr": "^19.0.5",
    "express": "^4.18.2",
    "firebase": "^12.2.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.5",
    "@angular/cli": "^19.0.5",
    "@angular/compiler-cli": "^19.0.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "angular-eslint": "20.3.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.35.0",
    "jasmine-core": "~5.4.0",
    "json-server": "^1.0.0-beta.3",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.0.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "8.40.0"
  }
}